import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc=new Scanner(System.in);
        int num1=sc.nextInt();
         int num2=sc.nextInt();
        char op = sc.next().charAt(0);
        switch(op){
            case '+':
                float sum=(float)num1+num2;
                System.out.printf("Addition of two number is %.1f",sum);
                    break;
            
                    case '-':
                float sub=(float)num1-num2;
                System.out.printf("Subtraction of two number is %.1f",sub);
                    break;
                
                    case '*':
                float mul=(float)num1*num2;
                System.out.printf("Multiplication of two number is %.1f",mul);
                    break;
                
                    case '/':
                float div=(float)num1/num2;
                System.out.printf("Division of two number is %.1f",div);
                    break;
                
                 case '%':
                 float mod=(float)num1%num2;
                System.out.printf("Modulo of two number is %.1f",mod);
                    break;
                
            default:
                 System.out.print("Invalid Input");
                
                
                
        }
        
    }
}
